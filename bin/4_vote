#!/bin/bash
#
# Output a script divide up the voting across subjects
#
# Usage: 
#     4_vote <vote.py options>
#
for subject in input/subjects/brains/*.mnc; do
    stem=$(basename $subject .mnc)
    if [ ! -e output/fusion/majvote/$stem/labels.mnc -a ! -e output/fusion/majvote/${stem}_labels.mnc ]; then
        echo vote.py $@ $stem 
    fi 
done > 4_vote
